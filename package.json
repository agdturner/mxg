{
  "name": "mxg",
  "version": "0.16",
  "license": "MIT",
  "description": "MESMER XML GUI.",
  "keywords": [
    "reaction",
    "kinetics",
    "chemistry"
  ],
  "source": "src/index.html",
  "homepage": "https://agdturner.github.io/mxg/",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "configure": "node configure.js",
    "build": "parcel build src/index.html --dist-dir dist/0.16",
    "start": "npx parcel src/index.html --dist-dir dist/0.16 -p 3460"
    },
  "targets": {
    "0.16": {
      "context": "browser",
      "includeNodeModules": true,
      "sourceMap": true,
      "engines": {
        "browsers": [
          "last 2 Chrome versions",
          "last 2 Firefox versions",
          "last 2 Safari versions",
          "last 2 Edge versions"
        ]
      },
      "distDir": "dist/0.16",
      "publicUrl": "/mxg/dist/0.16"
    }
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.13.2",
    "@parcel/transformer-webmanifest": "^2.13.2",
    "@types/big.js": "^6.2.2",
    "@types/node": "^22.10.1",
    "3dmol": "^2.4.2",
    "big.js": "^6.2.2",
    "dotenv": "^16.4.7",
    "parcel": "^2.13.2",
    "svgo": "^3.3.2",
    "typescript": "^5.7.2"
  },
  "author": {
    "name": "Struan Robertson",
    "url": "https://github.com/struanr"
  },
  "contributors": [
